{
    "name": "Ladybucks"
    , "icons": {
        "48": "icon-48.png"
        , "64": "icon-64.png"
    }
    , "permissions": [
        "tabs"
        , "http://*.wifi.olleh.com/*"
        , "https://*.wifi.olleh.com/*"
    ]
    , "content_scripts": [
        {
            "matches": ["http://first.wifi.olleh.com/starbucks/index_new.html"]
            , "js": ["redirect.js"]
            , "all_frames" : true
        }
        , {
            "matches": ["https://sms.wifi.olleh.com/web/memconf/starbucks/login.jsp"]
            , "js": [
                "ext/jquery-1.7.1.js"
                , "ext/underscore.js"
                , "ext/backbone.js"
                , "ext/backbone.local-storage.js"
                , "who.js"
                , "auto.js"
            ]
            , "all_frames" : true
            , "run_at" : "document_end"
        }
    ]
    ,   "page_action": {
        "default_name": "ladybucks"
        , "default_icon": "icon-16.png"
    }
    , "version": "0.3"
    , "description": "Automatically login to Starbucks WI-FI"
    , "options_page": "options.html"
    , "background_page": "background.html"
}
